!-- Xft settings -- !
Xft.antialias:	1
Xft.hinting:	1
Xft.autohint:	0
Xft.hintstyle:	hintslight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault
Xft.dpi:   100

Xcursor.theme: Bibata-Modern-Classic
Xcursor.size: 20

st.font:         UbuntuMono Nerd Font:style:medium:pixelsize=20

! window border inner
st.opacity: 255
st.borderpx: 10

